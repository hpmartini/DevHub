# Gemini AI API Key for configuration analysis
GEMINI_API_KEY=your_gemini_api_key_here

# code-server password for VS Code in browser (Phase 2 Web IDE)
# SECURITY: Set a strong, unique password before running code-server
# This password protects access to your entire codebase via the browser
# REQUIRED: You must set this to a secure password - do not use defaults!
CODE_SERVER_PASSWORD=

# code-server URL (optional, defaults to http://localhost:8443)
# Change protocol to https:// if running behind SSL proxy
VITE_CODE_SERVER_URL=http://localhost:8443

# PostgreSQL password
POSTGRES_PASSWORD=postgres
