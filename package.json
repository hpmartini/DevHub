{
  "name": "devorbit-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "postinstall": "node scripts/patch-node-pty.cjs",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run code-server\"",
    "client": "vite",
    "server": "node server/index.js",
    "code-server": "code-server --bind-addr 127.0.0.1:8080",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md}\"",
    "pty-host": "node pty-host/index.js",
    "pty-host:install": "cd pty-host && npm install",
    "dev:infra": "docker compose -f docker-compose.dev.yml up -d",
    "dev:full": "npm run dev:infra && npm run dev",
    "dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:start": "npm run pty-host:install && concurrently \"npm run pty-host\" \"docker compose up --build\"",
    "docker:logs": "docker compose logs -f",
    "electron:dev": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "bundle:server": "node scripts/bundle-server.js",
    "electron:rebuild": "electron-rebuild",
    "electron:build": "npm run build && npm run bundle:server && npm run electron:rebuild && electron-builder",
    "electron:build:mac": "npm run build && npm run bundle:server && npm run electron:rebuild && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:build:all": "npm run build && electron-builder --mac --win --linux",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-virtual": "^3.13.15",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "electron-updater": "^6.7.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.562.0",
    "node-pty": "^1.1.0",
    "pg": "^8.16.3",
    "pidusage": "^4.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-resizable-panels": "^4.2.2",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "tree-kill": "^1.2.2",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.3.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@electron/rebuild": "^4.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  }
}
